# dark mode
set recolor true
map <C-i> recolor

# enable copy to clipboard
set selection-clipboard clipboard

# page down/up
map [normal] d scroll full-down
map [normal] u scroll full-up

# scroll
map [normal] j scroll left
map [normal] k scroll down
map [normal] l scroll up
map [normal] / scroll right
# page navigation
map [normal] K navigate next
map [normal] L navigate previous
